#include <stdio.h>
 
int main() {
 
    int hora_inicial, hora_final,duracao;
    
    scanf("%d",&hora_inicial);
    scanf("%d",&hora_final);
    
    
    if(hora_inicial >=hora_final){
       
       duracao =(24 - hora_inicial)+ hora_final;
       printf("%d",duracao);
    }
    {
        duracao = hora_final - hora_inicial;
        printf("%d",duracao);
    }
 
    return 0;
}


#include <stdio.h>
 
int main() {
    
    int vi, vf;
 
  scanf("%d", &vi);
  scanf("%d", &vf);
  
  if(vi < vf){
      printf("O JOGO DUROU %d HORA(S)\n",vi - vf);
  }
  
  if(!(vi < vf)){
       printf("O JOGO DUROU %d HORA(S)\n",24-vi + vf);
  }
 
    return 0;
}
