#include <stdio.h>
 
int main() {
 
    int hora_inicial, hora_final,duracao;
    
    scanf("%d",&hora_inicial);
    scanf("%d",&hora_final);
    
    
    if(hora_inicial >=hora_final){
       
       duracao =(24 - hora_inicial)+ hora_final;
       printf("%d",duracao);
    }
    {
        duracao = hora_final - hora_inicial;
        printf("%d",duracao);
    }
 
    return 0;
}
